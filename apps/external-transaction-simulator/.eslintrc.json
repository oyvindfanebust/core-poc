{
  "extends": ["next/core-web-vitals", "@core-poc/eslint-config"],
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.json"
      }
    }
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
    "import/no-unresolved": ["error", { "ignore": ["^@/"] }]
  }
}
